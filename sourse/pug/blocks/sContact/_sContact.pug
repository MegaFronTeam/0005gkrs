mixin sContact()
	// start sContact
	+b.SECTION.sContact.section#sContact&attributes(attributes)
		.container
			+b.section-title
				h1 Контакты
			+e.container
				+e.map-wrap
					<script type="text/javascript" charset="utf-8" async src="https://api-maps.yandex.ru/services/constructor/1.0/js/?um=constructor%3A77e959ef2fb061d43b0c0dd7ef7342a55caaf48b58189049491d8668b48eb7f8&amp;width=100%25&amp;height=400&amp;lang=ru_RU&amp;scroll=false"></script>
				+e.address-wrap
					dl
						+e.dl-group
							dt Адрес
							dd Санкт-Петербург, ул. Такая-то, 134, оф. 105
						+e.dl-group
							dt Режим работы
							dd Пн-Вс с 9.00 до 18.00
						+e.dl-group
							dt Телефоны
							dd
								a(href="#") +7-800-000-00-00
								+soc()
								a(href="#") +7-800-000-00-01
						+e.dl-group
							dt Email
							dd gkrs@gmail.com
						+e.dl-group
							dt Наши реквизиты
							dd ИНН
							dd КПП
							dd ОГРН
							dd Расчетный счёт
							dd Название банка
							dd БИК
							dd Генеральный директор


			.row
	// end sContact